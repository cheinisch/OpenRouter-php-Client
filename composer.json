{
  "name": "cheinisch/openrouter-php-client",
  "description": "Modern, lightweight PHP client for the OpenRouter.ai API. Provides a simple interface for chat completions, text generation, and custom LLM endpoints.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "openrouter",
    "php",
    "llm",
    "ai",
    "client",
    "chat-completions"
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/cheinisch"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenRouter\\": "src-legacy/",
      "cheinisch\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  }
}
